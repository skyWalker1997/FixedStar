        <ExampleOutput>64.12.97.10</ExampleOutput>
        <TimeOut>20</TimeOut>
        <SiteStatus>Available</SiteStatus>
        <LastCheckDate>2018-8-14</LastCheckDate>
    </Site>
    <Site name="ThatsThem">
        <Request_Type>GET</Request_Type>
        <GetInfoType>Personal</GetInfoType>
            <entry>Name</entry>
            <entry>Email</entry>
            <entry>Address</entry>
            <entry>Age</entry>
            <entry>Phone</entry>
            <entry>IP</entry>
        <SiteIllustrate>Give a personal information,get personal information about the info you give</SiteIllustrate>
        <SiteUrl>https://thatsthem.com/email/</SiteUrl>
