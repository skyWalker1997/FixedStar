    <Site name="Email-Format">
        <Request_Type>GET</Request_Type>
        <GetInfoType>Email</GetInfoType>
        <SiteIllustrate>Give a domain,get Emails about this doumain</SiteIllustrate>
        <SiteUrl>https://www.email-format.com/d/</SiteUrl>
        <Regex>
            <![CDATA[email'.*?data-id=.*?fl'>(.*?)</div>]]>
        </Regex>
        <ExampleInput type="Domain">google.com</ExampleInput>
        <ExampleOutput>@google.com</ExampleOutput>
        <TimeOut>20</TimeOut>
        <SiteStatus>Available</SiteStatus>
        <LastCheckDate>2018-8-14</LastCheckDate>
    </Site>
    <Site name="Reversegenie">
        <Request_Type>GET</Request_Type>
        <GetInfoType>Personal</GetInfoType>
            <entry>Phone</entry>
            <entry>Address</entry>
            <entry>Gender</entry>
            <entry>IP</entry>
